{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "global-require": "off",
    "import/prefer-default-export": "off",
    "react/jsx-filename-extension": "off",
    "react/no-multi-comp": "off"
  },
  "extends": "airbnb",
  "plugins": [
    "react"
  ]
}
